{
  "name": "leaf-theme",
  "displayName": "Leaf Theme",
  "version": "0.1.0",
  "packageManager": "pnpm@7.5.0",
  "description": "Leaf PHP theme for VS Code",
  "categories": [
    "Themes"
  ],
  "keywords": [
    "theme",
    "leaf",
    "light",
    "dark"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mychidarko/vscode-theme-leaf"
  },
  "files": [
    "themes"
  ],
  "bugs": {
    "url": "https://github.com/mychidarko/vscode-theme-leaf/issues"
  },
  "publisher": "leafphp",
  "scripts": {
    "build": "esno src/index.ts",
    "dev": "nodemon --watch src -e ts --exec \"esno src/index.ts\"",
    "vscode:prepublish": "npm run build",
    "lint": "eslint .",
    "release": "npm run build && npx bumpp --commit --tag --push --all && vsce publish --no-dependencies"
  },
  "contributes": {
    "themes": [
      {
        "label": "Leaf Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/leaf-dark.json"
      },
      {
        "label": "Leaf Black",
        "uiTheme": "vs-dark",
        "path": "./themes/leaf-black.json"
      },
      {
        "label": "Leaf Dark Soft",
        "uiTheme": "vs-dark",
        "path": "./themes/leaf-dark-soft.json"
      }
    ]
  },
  "devDependencies": {
    "@antfu/utils": "^0.5.0",
    "@types/color": "^3.0.3",
    "@types/node": "^17.0.23",
    "color": "^4.2.3",
    "eslint": "^8.12.0",
    "esno": "^0.14.1",
    "nodemon": "^2.0.15",
    "tsup": "^5.12.4",
    "typescript": "^4.6.3",
    "vsce": "^2.7.0"
  },
  "engines": {
    "vscode": "^1.43.0"
  },
  "icon": "icon.png",
  "preview": true
}
